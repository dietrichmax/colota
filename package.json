{
  "name": "colota",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "lint": "npm run lint -w @colota/mobile",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "npm run test -w @colota/mobile",
    "test:coverage": "npm run test:coverage -w @colota/mobile",
    "android": "npm run android -w @colota/mobile",
    "start": "npm run start -w @colota/mobile",
    "clean": "npm run clean -w @colota/mobile"
  },
  "devDependencies": {
    "prettier": "^2.8.8"
  },
  "engines": {
    "node": ">=20"
  }
}
