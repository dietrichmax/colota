{
  "name": "Colota",
  "version": "1.3.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage",
    "clean": "cd android && gradlew clean && cd ..",
    "dev-android": "npx react-native run-android",
    "start": "react-native start",
    "android:debug": "cd android && gradlew assembleDebug || gradlew assembleDebug && cd ..",
    "android:release": "cd android && gradlew assembleRelease || ./gradlew assembleRelease && cd ..",
    "android:bundle": "cd android && gradlew bundleRelease || ./gradlew assembleRelease && cd ..",
    "deploy-release": "adb install android/app/build/outputs/apk/release/app-release.apk",
    "deploy-bundle": "adb install android/app/build/outputs/apk/bundle/app-release.aab"
  },
  "dependencies": {
    "@react-navigation/native": "^7.1.26",
    "@react-navigation/native-stack": "^7.9.0",
    "react": "19.2.0",
    "react-native": "0.83.1",
    "react-native-permissions": "^5.4.4",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.19.0",
    "react-native-webview": "^13.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-icons/all-files": "https://github.com/react-icons/react-icons/releases/download/v5.5.0/react-icons-all-files-5.5.0.tgz",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.83.1",
    "@react-native/eslint-config": "0.83.1",
    "@react-native/metro-config": "0.83.1",
    "@react-native/typescript-config": "0.83.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.2.0",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "^2.8.8",
    "react-test-renderer": "19.2.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}
