{
  "name": "@colota/mobile",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android --mode gmsDebug --appIdSuffix \"\"",
    "android:foss": "react-native run-android --mode fossDebug --appIdSuffix \"\"",
    "android:debug": "cd android && gradlew assembleGmsDebug",
    "android:debug:foss": "cd android && gradlew assembleFossDebug",
    "android:release": "cd android && gradlew assembleGmsRelease",
    "android:release:foss": "cd android && gradlew assembleFossRelease",
    "android:release:all": "cd android && gradlew assembleGmsRelease assembleFossRelease",
    "android:bundle": "cd android && gradlew bundleGmsRelease",
    "clean": "cd android && gradlew clean",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage",
    "deploy-release": "adb install android/app/build/outputs/apk/gms/release/app-gms-arm64-v8a-release.apk",
    "deploy-release:foss": "adb install android/app/build/outputs/apk/foss/release/app-foss-arm64-v8a-release.apk",
    "upload:playstore": "cd android && fastlane supply --track production --skip_upload_apk --skip_upload_aab"
  },
  "dependencies": {
    "@colota/shared": "*",
    "@maplibre/maplibre-react-native": "^10.4.2",
    "@react-navigation/native": "^7.1.31",
    "@react-navigation/native-stack": "^7.14.2",
    "lucide-react-native": "^0.564.0",
    "react": "19.2.0",
    "react-native": "0.83.1",
    "react-native-safe-area-context": "^5.7.0",
    "react-native-screens": "^4.24.0",
    "react-native-svg": "^15.15.3"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@babel/runtime": "^7.28.6",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.83.1",
    "@react-native/eslint-config": "0.83.1",
    "@react-native/metro-config": "0.83.1",
    "@react-native/typescript-config": "0.83.1",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.2.14",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "react-test-renderer": "19.2.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}
